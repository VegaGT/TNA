<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3.dtd">
<mapper namespace="com.oracle.tna.user.dao.ScoreListMapper">

	
	<select id="select" resultType="score">
		select data,sum(score) from score where USRE_ID = #{uid} group by data
	</select>

</mapper>